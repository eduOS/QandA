		} else {
			$('#pluckLogin').html('<a href="#" id="pLogin">Log in</a> | <a href="http://www2b.abc.net.au/communities/asp/register.asp?from=http://www.abc.net.au/tv/qanda/" id="pJoin">Join</a>');
		}
	}
	$('#pLoginBox').append('<a href="http://www2b.abc.net.au/communities/asp/forgot.asp?detail=pwd&proceed=y">Forgot password</a>');
	$('#pLoginBox').append('<a href="http://www2b.abc.net.au/communities/asp/forgot.asp?detail=login&proceed=y&gobacktitle=Login">Forgot username</a>');
	$('#pJoin').attr('href', 'http://www2b.abc.net.au/communities/asp/register.asp?from='+theUrl);
	$('#abc_pluck-login-form-submit').val('Log in');
	
	$("#pLogin").click(function () {
