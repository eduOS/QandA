		return false;
	});
	

	
});


</script>
	

<script language="JavaScript" type="text/javascript">


$(document).ready(function(){

	checkMonthsEnabled('2008');
	checkMonthsEnabled('2009');
	checkMonthsEnabled('2010');
	checkMonthsEnabled('2011');
	checkMonthsEnabled('2012');
	checkMonthsEnabled('2013');
	checkMonthsEnabled('2014');
	
	/* When the filter is clicked */
	$('#filter a').click(function() {
		/* remove the active class */
		$('#filter li.active').removeClass('active');
		/* add the active class to the clicked element */
		$(this).parent().addClass('active');
		/* if this href is all */
		if ($(this).attr('href') == '#all2008') {
			/* show all the elements for this year */
			showAllYear('2008');
		}
		else 
			if ($(this).attr('href') == '#all2009') {
				showAllYear('2009');
			}
		else 
			if ($(this).attr('href') == '#all2010') {
				showAllYear('2010');
			}
		else 
			if ($(this).attr('href') == '#all2011') {
				showAllYear('2011');
			}
		else 
			if ($(this).attr('href') == '#all2012') {
				showAllYear('2012');
			}
		else 
			if ($(this).attr('href') == '#all2013') {
				showAllYear('2013');
			}
		else 
			if ($(this).attr('href') == '#all2014') {
				showAllYear('2014');
			}			
			else {
				/* hide all the elements */
				$('#filter-items > li').hide();
				/* fade in the target */
				$('#filter-items #' + $(this).attr('href').substr(1)).fadeIn('slow');
			} 
		/* return false on the click */
		return false;
	});
		
	/* When the filterYear is clicked */
	$('#filterYear a').click(function() {
		/* remove the active class */
		$('#filterYear li.active').removeClass('active');
		/* add the active class to the clicked element */
		$(this).parent().addClass('active');
		/* hide all years */
		$('#filter-items > li').hide();
		$('#filter > ul').hide();
		/* make relevant year visible */
		$('#filter #' + $(this).attr('href').substr(1)).show();
		showAllYear($(this).attr('href').substr(1));
		return false;
	});

	// click 2014 (quick workaround for bug where all years are shown on load)
	$('#click2014').click();
	
// end ready
});




function showAllYear(yearString) {
	$('#filter-items > li').hide();
	$('#filter-items #January' + yearString).fadeIn('slow');
	$('#filter-items #February' + yearString).fadeIn('slow');
	$('#filter-items #March' + yearString).fadeIn('slow');
	$('#filter-items #April' + yearString).fadeIn('slow');
	$('#filter-items #May' + yearString).fadeIn('slow');
	$('#filter-items #June' + yearString).fadeIn('slow');
	$('#filter-items #July' + yearString).fadeIn('slow');
	$('#filter-items #August' + yearString).fadeIn('slow');
	$('#filter-items #September' + yearString).fadeIn('slow');
	$('#filter-items #October' + yearString).fadeIn('slow');
	$('#filter-items #November' + yearString).fadeIn('slow');
	$('#filter-items #December' + yearString).fadeIn('slow');
}

function checkMonthsEnabled(yearString) {
	if($('#filter-items #January' + yearString).length != 0) {$('#filterJanuary' + yearString).html('<a href="#January' + yearString + '">Jan</a>')}
	if($('#filter-items #February' + yearString).length != 0) {$('#filterFebruary' + yearString).html('<a href="#February' + yearString + '">Feb</a>')}
	if($('#filter-items #March' + yearString).length != 0) {$('#filterMarch' + yearString).html('<a href="#March' + yearString + '">Mar</a>')}
	if($('#filter-items #April' + yearString).length != 0) {$('#filterApril' + yearString).html('<a href="#April' + yearString + '">Apr</a>')}
	if($('#filter-items #May' + yearString).length != 0) {$('#filterMay' + yearString).html('<a href="#May' + yearString + '">May</a>')}
	if($('#filter-items #June' + yearString).length != 0) {$('#filterJune' + yearString).html('<a href="#June' + yearString + '">Jun</a>')}
	if($('#filter-items #July' + yearString).length != 0) {$('#filterJuly' + yearString).html('<a href="#July' + yearString + '">Jul</a>')}
	if($('#filter-items #August' + yearString).length != 0) {$('#filterAugust' + yearString).html('<a href="#August' + yearString + '">Aug</a>')}
	if($('#filter-items #September' + yearString).length != 0) {$('#filterSeptember' + yearString).html('<a href="#September' + yearString + '">Sep</a>')}
	if($('#filter-items #October' + yearString).length != 0) {$('#filterOctober' + yearString).html('<a href="#October' + yearString + '">Oct</a>')}
	if($('#filter-items #November' + yearString).length != 0) {$('#filterNovember' + yearString).html('<a href="#November' + yearString + '">Nov</a>')}
	if($('#filter-items #December' + yearString).length != 0) {$('#filterDecember' + yearString).html('<a href="#December' + yearString + '">Dec</a>')}
}



</script>


	
